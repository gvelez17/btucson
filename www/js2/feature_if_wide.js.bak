
w = 0;
if (window.width) {
	w = window.width;
} else if (window.innerWidth) {
	w = window.innerWidth;
} else if (document.body.clientWidth) {
	w = document.body.clientWidth;
}
if (w > 1055) {
	document.write('<span style="width:200px; float:right; background:#eee0e0;">');
	document.write('[|FEATURE|]'); 
	document.write('</span>');
}
